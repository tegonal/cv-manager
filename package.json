{
  "name": "cv-manager",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "cross-env NODE_OPTIONS=--no-deprecation next dev",
    "dev:mongodb": "env-cmd -f .env.local_mongodb -- yarn run dev",
    "dev:postgres": "env-cmd -f .env.local_postgres -- yarn run dev",
    "dev:sqlite": "env-cmd -f .env.local_sqlite -- yarn run dev",
    "services:start": "docker compose -f docker-compose.dev.yml up -d --remove-orphans",
    "services:stop": "docker compose -f docker-compose.dev.yml down --remove-orphans",
    "services:destroy": "docker compose -f docker-compose.dev.yml down --volumes --remove-orphans",
    "devsafe": "rm -rf .next && cross-env NODE_OPTIONS=--no-deprecation next dev",
    "build": "cross-env NODE_OPTIONS=--no-deprecation next build -d",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "start": "cross-env NODE_OPTIONS=--no-deprecation payload migrate && cross-env NODE_OPTIONS=--no-deprecation next start",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation next lint --fix && yarn run prettier",
    "prettier": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css}\"",
    "check-lint": "cross-env NODE_OPTIONS=--no-deprecation next lint",
    "migrate:create": "cross-env NODE_OPTIONS=--no-deprecation payload migrate:create && next lint --fix",
    "migrate:create:mongodb": "env-cmd -f .env.local_mongodb -- yarn run migrate:create",
    "migrate:create:postgres": "env-cmd -f .env.local_postgres -- yarn run migrate:create",
    "migrate:create:sqlite": "env-cmd -f .env.local_sqlite -- yarn run migrate:create",
    "migrate:create:all": "yarn run migrate:create:postgres && yarn run migrate:create:sqlite",
    "migrate": "cross-env NODE_OPTIONS=--no-deprecation payload migrate",
    "generate:types": "payload generate:types && yarn run lint",
    "generate:importMap": "payload generate:importMap && yarn run lint",
    "standalone-script": "tsx ./src/scripts/standalone-script.ts",
    "up": "yarn upgrade-interactive",
    "self-update": "yarn set version latest && yarn"
  },
  "engines": {
    "node": ">=22.14.0"
  },
  "dependencies": {
    "@payloadcms/db-mongodb": "3.35.1",
    "@payloadcms/db-postgres": "3.35.1",
    "@payloadcms/db-sqlite": "3.35.1",
    "@payloadcms/email-nodemailer": "3.35.1",
    "@payloadcms/next": "3.35.1",
    "@payloadcms/richtext-lexical": "3.35.1",
    "@payloadcms/storage-s3": "3.35.1",
    "@payloadcms/ui": "3.35.1",
    "babel-plugin-react-compiler": "19.0.0-beta-ebf51a3-20250411",
    "graphql": "^16.10.0",
    "jsonwebtoken": "^9.0.2",
    "ky": "^1.8.1",
    "lodash-es": "^4.17.21",
    "next": "15.3.1",
    "nodemailer": "^6.10.1",
    "pagedjs": "^0.4.3",
    "payload": "3.35.1",
    "payload-oauth2": "^1.0.14",
    "puppeteer": "^24.6.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-i18next": "^15.4.1",
    "sharp": "0.34.1",
    "tslog": "^4.9.3"
  },
  "devDependencies": {
    "@swc/core": "1.11.21",
    "@tailwindcss/typography": "^0.5.16",
    "@types/jsdom": "^21.1.7",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^22.14.1",
    "@types/nodemailer": "^6.4.17",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@typescript-eslint/eslint-plugin": "8.30.1",
    "@typescript-eslint/parser": "8.30.1",
    "autoprefixer": "^10.4.21",
    "copy-webpack-plugin": "^13.0.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.5.0",
    "env-cmd": "^10.1.0",
    "eslint": "^9.25.0",
    "eslint-config-next": "15.3.1",
    "eslint-plugin-jsonc": "^2.20.0",
    "postcss": "^8.5.3",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "tailwindcss": "^3.4.17",
    "typescript": "5.8.3"
  },
  "packageManager": "yarn@4.9.1"
}
